<div class="modal fade" id="deleteModal" tabindex="-1" role="dialog" aria-labelledby="deleteModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="deleteModalLabel">{{#str}}delete, local_roomsupport{{/str}} <span id="delete-name"></span></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="card">
                    <div class="card-body">                        
                        <div id="delete-content"></div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-danger" id="deleteBtn">{{#str}}delete, local_roomsupport{{/str}}</button>
                <button type="button" class="btn btn-outline-primary" data-dismiss="modal">{{#str}}cancel, local_roomsupport{{/str}}</button>
            </div>
        </div>
    </div>
</div> 